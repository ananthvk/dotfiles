{
    "vim.easymotion": true,
    "vim.incsearch": true,
    "vim.useSystemClipboard": true,
    "vim.useCtrlKeys": true,
    "vim.hlsearch": true,
    "vim.insertModeKeyBindings": [
        {
            "before": [
                "j",
                "k"
            ],
            "after": [
                "<Esc>"
            ]
        }
    ],
    "vim.normalModeKeyBindingsNonRecursive": [
        {
            "before": [
                "<leader>",
                "d"
            ],
            "after": [
                "d",
                "d"
            ]
        },
        {
            "before": [
                "<C-n>"
            ],
            "commands": [
                ":nohl"
            ]
        }
    ],
    "vim.leader": "<space>",
    "vim.handleKeys": {
        "<C-a>": false,
        "<C-f>": false
    },
    "[c]": {
        "editor.defaultFormatter": "ms-vscode.cpptools"
    },
    "C_Cpp.default.includePath": [
        "$(default}",
        "/usr/include",
        "/usr/include/arpa",
        "/usr/include/netinet",
        "/usr/local/include"
    ],
    "C_Cpp.default.compilerPath": "/usr/bin/gcc",
    "C_Cpp.default.cppStandard": "c++20",
    "C_Cpp.default.cStandard": "c11",
    "C_Cpp.default.intelliSenseMode": "linux-clang-x64",
    "C_Cpp.formatting": "clangFormat",
    "workbench.colorTheme": "Gruvbox Material Dark",
    "terminal.integrated.enableMultiLinePasteWarning": "never",
    "editor.fontFamily": "'DejaVuSansM Nerd Font Mono','IBM Plex Mono', 'Inter', 'Source Code Pro Medium', 'Iosevka NFM',   'Roboto'",
    "notebook.output.fontFamily": "'IBM Plex Mono'",
    // "terminal.integrated.fontFamily": "'Iosevka NFM'",
    "editor.inlayHints.fontFamily": "'Inter'",
    "editor.inlineSuggest.fontFamily": "'Inter'",
    "markdown.preview.fontFamily": "'IBM Plex Mono'",
    "redhat.telemetry.enabled": false,
    "workbench.iconTheme": "material-icon-theme",
    "editor.detectIndentation": false,
    "C_Cpp.loggingLevel": "Debug",
    "C_Cpp.default.defines": [
        "_POSIX_C_SOURCE=200809L"
    ],
    "extensions.experimental.affinity": {
        "asvetliakov.vscode-neovim": 1
    },
    "vscode-neovim.neovimExecutablePaths.linux": "/usr/bin/nvim",
    "vscode-neovim.neovimInitVimPaths.linux": "/home/shank/.config/nvim/init.lua",
    "github.copilot.enable": {
        "*": false,
        "plaintext": false,
        "markdown": false,
        "scminput": false
    },
    // "editor.formatOnSave": true,
    // "editor.formatOnSaveMode": "modificationsIfAvailable",
    "python.analysis.packageIndexDepths": [
        {
            "name": "django",
            "depth": 5
        }
    ],
    "[python]": {
        "diffEditor.ignoreTrimWhitespace": false
    },
    "pylint.args": [
        "--load-plugins pylint_django"
    ],
    "haskell.manageHLS": "GHCup",
    "dart.flutterSdkPath": "/home/shank/.fluttersdk/flutter",
    "[rust]": {},
    "mesonbuild.downloadLanguageServer": true,
    "svelte.enable-ts-plugin": true,
    "settingsSync.ignoredExtensions": [
        "ms-ossdata.vscode-postgresql"
    ],
    "workbench.editor.enablePreview": false,
    "leetcode.workspaceFolder": "/home/shank/code/leetcode",
    "vscode-neovim.compositeKeys": {
        "jk": {
            "command": "vscode-neovim.escape"
        },
        "jj": {
            "command": "vscode-neovim.escape"
        }
    },
    "workbench.layoutControl.enabled": false,
    "explorer.openEditors.minVisible": 0,
    "explorer.autoReveal": false,
    "workbench.sideBar.location": "right",
    "editor.minimap.enabled": false,
    "workbench.tree.indent": 12,
    "workbench.tree.renderIndentGuides": "none",
    "workbench.editor.tabCloseButton": "off",
    "editor.overviewRulerBorder": false,
    "editor.padding.bottom": 14,
    "editor.padding.top": 14,
    "editor.scrollbar.horizontal": "hidden",
    "editor.codeLens": false,
    "editor.foldingHighlight": false,
    "editor.renderLineHighlight": "gutter",
    "editor.guides.indentation": true,
    "editor.guides.highlightActiveIndentation": false,
    "editor.matchBrackets": "never",
    "editor.guides.highlightActiveBracketPair": false,
    "editor.parameterHints.enabled": false,
    "editor.inlayHints.enabled": "off",
    "editor.smoothScrolling": true,
    "editor.inlineSuggest.enabled": false,
    "editor.lineNumbers": "relative",
    "explorer.compactFolders": false,
    "window.autoDetectHighContrast": false,
    "zenMode.fullScreen": true,
    "editor.fontSize": 16,
    "editor.fontWeight": "400",
    "editor.lineHeight": 26,
    "debug.toolBarLocation": "docked",
    "diffEditor.ignoreTrimWhitespace": false,
    "diffEditor.renderSideBySide": false,
    "editor.bracketPairColorization.enabled": false,
    "editor.colorDecorators": false,
    "editor.cursorStyle": "line-thin",
    "editor.fontLigatures": true,
    "editor.formatOnSave": false,
    "editor.gotoLocation.multipleDeclarations": "goto",
    "editor.gotoLocation.multipleDefinitions": "goto",
    "editor.gotoLocation.multipleImplementations": "goto",
    "editor.gotoLocation.multipleReferences": "goto",
    "editor.gotoLocation.multipleTypeDefinitions": "goto",
    "editor.hideCursorInOverviewRuler": true,
    "editor.renderWhitespace": "none",
    "editor.scrollbar.horizontalScrollbarSize": 8,
    "editor.scrollbar.verticalScrollbarSize": 8,
    "explorer.decorations.badges": false,
    "git.enableSmartCommit": true,
    "git.confirmSync": false,
    "markdown.preview.markEditorSelection": false,
    "problems.decorations.enabled": false,
    "scm.diffDecorations": "none",
    "terminal.integrated.cursorStyle": "line",
    "terminal.integrated.fontSize": 14,
    "terminal.integrated.lineHeight": 1.45,
    "window.autoDetectColorScheme": true,
    "window.newWindowDimensions": "inherit",
    "window.title": "",
    "window.titleBarStyle": "custom",
    "workbench.editor.revealIfOpen": true,
    "workbench.editor.showTabs": "single",
    "workbench.list.horizontalScrolling": true,
    "workbench.preferredDarkColorTheme": "Gruvbox Material Dark",
    "workbench.preferredLightColorTheme": "Gruvbox Material Light",
    "workbench.startupEditor": "none",
    "workbench.statusBar.visible": false,
    "workbench.tips.enabled": false,
    "workbench.activityBar.location": "hidden",
    "workbench.editor.tabActionCloseVisibility": false,
    "editor.scrollbar.vertical": "hidden",
    "color-highlight.markRuler": false,
    "editor.stickyScroll.enabled": false,
    "breadcrumbs.enabled": false,
    "github.copilot.editor.enableAutoCompletions": true,
    "leetcode.defaultLanguage": "cpp",
    "leetcode.allowReportData": false,
    "leetcode.hint.commandShortcut": false,
    "leetcode.hint.configWebviewMarkdown": false,
    "github.copilot.chat.codesearch.enabled": true,
    "github.copilot.chat.agent.thinkingTool": true,
    "github.copilot.chat.editor.temporalContext.enabled": true,
    "github.copilot.chat.edits.temporalContext.enabled": true,
    "github.copilot.chat.generateTests.codeLens": true,
    "github.copilot.chat.languageContext.fix.typescript.enabled": true,
    "github.copilot.chat.languageContext.inline.typescript.enabled": true,
    "github.copilot.chat.languageContext.typescript.enabled": true,
    // To show debug breakpoints
    "editor.glyphMargin": true,
    "debug.openDebug": "openOnSessionStart",
    "chat.instructionsFilesLocations": {
        ".github/instructions": true,
        "/tmp/postman-http-request-post-response.instructions.md": true,
        "/tmp/postman-http-request-pre-request.instructions.md": true,
        "/tmp/postman-collections-post-response.instructions.md": true,
        "/tmp/postman-collections-pre-request.instructions.md": true,
        "/tmp/postman-folder-post-response.instructions.md": true,
        "/tmp/postman-folder-pre-request.instructions.md": true
    },
    "docker.extension.enableComposeLanguageServer": false,
    "github.copilot.nextEditSuggestions.enabled": true,
    "python.analysis.typeCheckingMode": "standard",
    "files.exclude": {
        "**/__pycache__": true
    },
    "notebook.output.textLineLimit": 80
}